{
  "Instructions": "Use this file as a template for your application and save it with the name appsetings.json. Write the appropriate values for ClientId, ClientSecret or ClientCertificate (use just one) and ContainerTypeId",
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "prompt": "select_account",
    "TenantId": "common",
    "ClientId": "[appId]",
    "CallbackPath": "/signin-oidc",
    "SignedOutCallbackPath ": "/signout-callback-oidc",
    "ClientSecret": "[client secret]",
    "_ClientCertificates": [
      {
        "SourceType": "Path",
        "CertificateDiskPath": "[path to combined cert pfx]"
      }
    ]
  },

  "GraphAPI": {
    "Endpoint": "https://graph.microsoft.com/v1.0",
    "StaticScope": "https://graph.microsoft.com/.default"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "TestContainer": {
    "ContainerTypeId": "[ContainerTypeId]"
  },
  "ConnectionStrings": {
    "AppDBConnStr": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=DemoAppDb;Integrated Security=True;Connect Timeout=30;"
  },
  "Urls": "https://localhost:57750"
}
